      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "uuid",
            "flow": ""
          }
        ],
        "decryption": "none",
        "fallbacks": []
      },
      "streamSettings": {
        "network": "xhttp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "www.yahoo.com:443",
          "xver": 0,
          "serverNames": [
            "www.yahoo.com"
          ],
          "privateKey": "prk",
          "publicKey": "puk",
          "shortIds": [
            "123abc"
          ],
          "fingerprint": "chrome"
        },
        "xhttpSettings": {
          "path": "/xhttp",
          "host": "",
          "headers": {},
          "scMaxBufferedPosts": 30,
          "scMaxEachPostBytes": "1000000",
          "noSSEHeader": false,
          "xPaddingBytes": "100-1000",
          "mode": "auto"
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}
## 配置详解
{
  "log": { // 日志配置部分
    "loglevel": "info"
  },
  "inbounds": [ // 入站连接配置，定义如何接收客户端连接
    {
      "port": 443, // 服务器监听的端口号
      "protocol": "vless", // 使用VLESS协议，这是一种轻量级的加密传输协议
      "settings": { // VLESS协议的具体设置
        "clients": [ // 允许连接的客户端列表
          {
            "id": "uuid", // 客户端的UUID，作为身份验证凭证
            "flow": "" // 流控模式，空字符串表示不启用流控
          }
        ],
        "decryption": "none", // VLESS本身不加密，加密由传输层处理
        "fallbacks": [] // 回退配置，为空表示不启用回退功能
      },
      "streamSettings": { // 传输层设置
        "network": "xhttp", // 使用xhttp作为传输协议，这是X-Ray的一种HTTP传输实现
        "security": "reality", // 使用REALITY作为安全层，这是一种高级的传输安全协议
        "realitySettings": { // REALITY协议的具体设置
          "show": false, // 是否显示REALITY详细信息，false表示不显示
          "dest": "www.yahoo.com:443", // 目标站点，用于伪装流量
          "xver": 0, // X-Forwarded-For头信息设置，0表示不发送
          "serverNames": [ // 用于SNI（服务器名称指示）的域名列表
            "www.yahoo.com" // 此处使用yahoo.com作为伪装站点
          ],
          "privateKey": "prk", // REALITY的私钥
          "publicKey": "puk", // REALITY的公钥
          "shortIds": [ // 短ID列表，用于REALITY协议的握手过程
            "a33f577d" // 一个短ID示例
          ],
          "fingerprint": "chrome" // 使用Chrome浏览器的TLS指纹进行伪装
        },
        "xhttpSettings": { // xhttp传输协议的具体设置
          "path": "/xhttp", // 服务器路径，客户端需要匹配此路径
          "host": "", // 主机名，为空表示不指定特定主机
          "headers": {}, // 额外的HTTP头信息，为空表示不添加
          "scMaxBufferedPosts": 30, // 最大缓存POST请求数量
          "scMaxEachPostBytes": "1000000", // 每个POST请求的最大字节数
          "noSSEHeader": false, // 是否禁用SSE头，false表示不禁用
          "xPaddingBytes": "100-1000", // 添加100-1000字节的随机填充，增加流量混淆效果
          "mode": "auto" // 自动模式，根据环境自动选择最佳传输方式
        }
      },
      "sniffing": { // 流量嗅探设置
        "enabled": true, // 启用嗅探功能
        "destOverride": ["http", "tls", "quic"] // 对http、tls和quic协议进行目标地址重写
      }
    }
  ],
  "outbounds": [ // 出站连接配置，定义如何发送流量
    {
      "protocol": "freedom", // 使用freedom协议，直接连接到目标地址
      "settings": {} // freedom协议的设置，空对象表示使用默认设置
    }
  ]
}
